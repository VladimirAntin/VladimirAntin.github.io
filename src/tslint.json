{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            ["app",""],
            ["camelCase", "kebab-case"]
        ],
        "component-selector": [
            true,
            "element",
            ["app",""],
            "kebab-case"
        ],
      "import-blacklist": [true]
    }
}
